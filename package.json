{
	"name": "js-browser-starter-kit",
	"version": "0.0.1",
	"author": "Grzegorz Przyby≈Çowicz",
	"license": "MIT",
	"scripts": {
		"prestart": "babel-node scripts/startMessage.js",
		"start": "npm-run-all --parallel security-check start:webServer lint:watch test:watch",
		"start:webServer": "babel-node scripts/webServer.js",
		"security-check": "nsp check",
		"lint": "esw --color webpack.config.* src build",
		"lint:watch": "npm run lint -- --watch",
		"test": "mocha scripts/testSetup.js \"src/**/*.test.js\"",
		"test:watch": "npm run test -- --watch",
		"clean-bin": "rimraf ./bin && mkdir bin",
		"prebuild": "npm-run-all clean-bin test lint",
		"build": "babel-node scripts/build.js"
	},
	"dependencies": {},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-core": "6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-latest": "6.24.1",
		"babel-register": "6.26.0",
		"chai": "4.1.2",
		"chalk": "2.4.1",
		"cheerio": "1.0.0-rc.2",
		"compression": "1.7.2",
		"cross-env": "5.1.4",
		"css-loader": "0.28.11",
		"eslint": "4.19.1",
		"eslint-plugin-import": "2.11.0",
		"eslint-watch": "3.1.4",
		"express": "4.16.3",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"html-webpack-plugin": "3.2.0",
		"jsdom": "11.10.0",
		"json-schema-faker": "0.5.0-rc15",
		"json-server": "0.12.2",
		"localtunnel": "1.9.0",
		"mocha": "5.1.1",
		"nock": "9.2.5",
		"node-sass": "4.9.0",
		"npm-run-all": "4.1.2",
		"nsp": "3.2.1",
		"numeral": "2.0.6",
		"open": "0.0.5",
		"rimraf": "2.6.2",
		"sass-loader": "7.0.1",
		"style-loader": "0.21.0",
		"surge": "0.20.1",
		"webpack": "4.6.0",
		"webpack-dev-middleware": "3.1.3",
		"webpack-hot-middleware": "2.22.1",
		"webpack-md5-hash": "0.0.6"
	}
}
