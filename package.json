{
	"name": "js-browser-starter-kit",
	"version": "0.0.1",
	"author": "Grzegorz Przyby≈Çowicz",
	"license": "MIT",
	"scripts": {
		"prestart": "babel-node scripts/startMessage.js",
		"start": "npm-run-all --parallel security-check start:webServer lint:watch test:watch",
		"start:webServer": "babel-node scripts/webServer.js",
		"security-check": "nsp check",
		"lint": "esw --color webpack.config.* src build",
		"lint:watch": "npm run lint -- --watch",
		"test": "mocha scripts/testSetup.js \"src/**/*.test.js\"",
		"test:watch": "npm run test -- --watch",
		"clean-bin": "rimraf ./bin && mkdir bin",
		"prebuild": "npm-run-all clean-bin test lint",
		"build": "babel-node scripts/build.js"
	},
	"dependencies": {},
	"devDependencies": {
		"babel-cli": "6.16.0",
		"babel-core": "6.17.0",
		"babel-loader": "^7.1.4",
		"babel-preset-latest": "6.16.0",
		"babel-register": "6.16.3",
		"chai": "3.5.0",
		"chalk": "1.1.3",
		"cheerio": "0.22.0",
		"compression": "1.6.2",
		"cross-env": "3.1.3",
		"css-loader": "0.25.0",
		"eslint": "3.8.1",
		"eslint-plugin-import": "2.0.1",
		"eslint-watch": "2.1.14",
		"express": "4.14.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-webpack-plugin": "3.2.0",
		"jsdom": "9.8.0",
		"json-schema-faker": "0.3.6",
		"json-server": "0.8.22",
		"localtunnel": "1.8.1",
		"mocha": "3.1.2",
		"nock": "8.1.0",
		"node-sass": "4.9.0",
		"npm-run-all": "3.1.1",
		"nsp": "2.6.2",
		"numeral": "1.5.3",
		"open": "0.0.5",
		"rimraf": "2.5.4",
		"sass-loader": "7.0.1",
		"style-loader": "0.13.1",
		"surge": "0.18.0",
		"webpack": "4.6.0",
		"webpack-dev-middleware": "3.1.3",
		"webpack-hot-middleware": "2.22.1",
		"webpack-md5-hash": "0.0.5"
	}
}
